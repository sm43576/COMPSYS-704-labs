<System xmlns="http://systemjtechnology.com">
	<SubSystem Name="SS2" Local="true">
		<ClockDomain Name="TemperaturePlantCD" Class="TemperaturePlant">
			<iSignal Name="getTemp1" Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="10001" />
			<iSignal Name="window1" Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="10001" />
			<oSignal Name="getTemp1R" To="TemperatureSensorCD.getTemp1R" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="10000" />
			<oSignal Name="openedWindow1" To="TemperatureSensorCD.openedWindow1" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="10000" />
		</ClockDomain>
		
		<ClockDomain Name="HumidityPlantCD" Class="HumidityPlant">
			<iSignal Name="getHumid4" Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="10001" />
			<iSignal Name="getHumid4R" Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="10001" />
			<iSignal Name="airconHumid4" Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="10001" />
			<oSignal Name="getHumid4R" To="HumiditySensorCD.getTemp1R" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="10000" />
		</ClockDomain>
	</SubSystem>
</System>
